{
  "name": "ns-white-crane-white-belt",
  "version": "0.1.0",
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/node": "24.7.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "dependency-cruiser": "17.0.2",
    "eslint": "9.36.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "jsdom": "27.0.0",
    "knip": "5.64.3",
    "npm-check-updates": "19.0.0",
    "openapi-typescript": "7.9.1",
    "prettier": "3.6.2",
    "quicktype": "23.2.6",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "syncpack": "14.0.0-alpha.19",
    "tsx": "4.20.6",
    "turbo": "2.5.8",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  },
  "packageManager": "npm@10.9.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "debug": "turbo debug",
    "dev": "turbo dev",
    "check:all": "turbo lint typecheck",
    "check:fast": "turbo lint:fix typecheck --continue --concurrency=4",
    "check:changed": "turbo lint:fix typecheck test build fmt --filter='[HEAD~1]' --continue --concurrency=4",
    "fix:changed": "turbo lint:fix fmt --filter='[HEAD~1]' --continue --concurrency=4",
    "clean": "turbo clean",
    "coverage": "turbo coverage",
    "coverage:vitest": "turbo coverage:vitest",
    "deps:check": "npm-check-updates --workspaces --dep prod,dev,optional",
    "deps:update": "npm-check-updates --workspaces -u -x eslint,@eslint/js,@vitest/eslint-plugin,@tanstack/react-start,eslint-plugin-react-hooks --dep prod,dev,optional",
    "fmt": "turbo fmt",
    "fmt:check": "turbo fmt:check",
    "fmt:check:prettier": "turbo fmt:check:prettier",
    "fmt:prettier": "turbo fmt:prettier",
    "lint": "turbo lint",
    "lint:eslint": "turbo lint:eslint",
    "lint:fix": "turbo lint:fix",
    "prepare": "husky",
    "test": "turbo test",
    "test:mutation": "turbo test:mutation",
    "test:vitest": "turbo test:vitest",
    "test:watch": "turbo test:watch",
    "typecheck": "turbo typecheck",
    "typecheck:tsc": "turbo typecheck:tsc"
  },
  "type": "module",
  "overrides": {
    "devtools-protocol": "0.0.1523305"
  },
  "workspaces": [
    "packages/shared/*",
    "packages/config/*",
    "packages/templates/*",
    "packages/katas/*"
  ]
}
