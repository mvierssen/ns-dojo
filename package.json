{
  "name": "ns-dojo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/config/*",
    "packages/shared/*",
    "packages/katas/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.3.0",
    "@commitlint/config-conventional": "20.3.0",
    "@moonrepo/cli": "^1.41.7",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/node": "25.0.3",
    "@vitest/coverage-istanbul": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "dependency-cruiser": "17.3.6",
    "eslint": "9.39.2",
    "jiti": "2.6.1",
    "jsdom": "27.4.0",
    "knip": "5.80.0",
    "npm-check-updates": "19.3.1",
    "prettier": "3.7.4",
    "quicktype": "23.2.6",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "syncpack": "14.0.0-alpha.19",
    "tsx": "4.21.0",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "scripts": {
    "deps:check": "npm-check-updates --workspaces --dep prod,dev,optional",
    "deps:update": "npm-check-updates --workspaces -u --dep prod,dev,optional",
    "moon": "moon",
    "build": "moon run :build",
    "test": "moon run :test",
    "lint": "moon run :lint",
    "lint:fix": "moon run :lint:fix",
    "fmt": "moon run :fmt",
    "fmt:check": "moon run :fmt:check",
    "typecheck": "moon run :typecheck",
    "coverage": "moon run :coverage",
    "dev": "moon run :dev",
    "clean": "moon run :clean",
    "check:all": "moon run :lint :typecheck",
    "check:fast": "moon run :lint:fix :typecheck --concurrency=4",
    "check:affected": "moon run :lint:fix :typecheck :test :build :fmt --affected --concurrency=4",
    "ci": "moon ci",
    "sync:hooks": "moon sync hooks"
  },
  "dependencies": {
    "keyv": "^5.5.5"
  }
}
