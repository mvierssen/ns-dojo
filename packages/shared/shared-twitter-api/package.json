{
  "name": "@ns-dojo/shared-twitter-api",
  "version": "0.1.0",
  "dependencies": {},
  "devDependencies": {
    "@ns-dojo/eslint-config-node": "0.1.0",
    "@ns-dojo/ts-config-lib-compiled": "0.1.0",
    "@ns-dojo/vitest-config-node": "0.1.0",
    "openapi-typescript": "7.10.1",
    "typescript": "5.9.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "source": "./src/*.ts",
      "import": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "files": [
    "package.json",
    "dist"
  ],
  "main": "./dist/src/index.js",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "coverage": "npm run coverage:vitest",
    "coverage:vitest": "vitest run --coverage",
    "fmt": "npm run fmt:prettier",
    "fmt:check": "npm run fmt:check:prettier",
    "fmt:check:prettier": "prettier --check .",
    "fmt:prettier": "prettier --write .",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "openapi:generate:web": "openapi-typescript \"https://raw.githubusercontent.com/konfig-sdks/openapi-examples/main/x/openapi.yaml\" -o ./src/api.d.ts",
    "openapi:generate": "openapi-typescript ./openapi.yaml -o ./src/api.d.ts",
    "test": "npm run test:vitest",
    "test:vitest": "vitest run",
    "typecheck": "npm run typecheck:tsc",
    "typecheck:tsc": "tsc --noemit"
  },
  "type": "module",
  "types": "./dist/src/src/index.d.ts"
}
