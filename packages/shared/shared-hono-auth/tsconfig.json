{
  "extends": "@ns-dojo/ts-config-lib-compiled",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": ".",
    "paths": {
      "@ns-dojo/shared-crypto": ["../shared-crypto/src/index.ts"],
      "@ns-dojo/shared-crypto/*": ["../shared-crypto/src/*"],
      "@ns-dojo/shared-hono": ["../shared-hono/src/index.ts"],
      "@ns-dojo/shared-hono/*": ["../shared-hono/src/*"],
      "@ns-dojo/shared-logging": ["../shared-logging/src/index.ts"],
      "@ns-dojo/shared-logging/*": ["../shared-logging/src/*"]
    }
  },
  "references": [
    {
      "path": "../../config/eslint-config-node"
    },
    {
      "path": "../shared-crypto"
    },
    {
      "path": "../shared-hono"
    },
    {
      "path": "../shared-logging"
    },
    {
      "path": "../../config/vitest-config-node"
    }
  ]
}
