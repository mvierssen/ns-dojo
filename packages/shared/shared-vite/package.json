{
  "name": "@ns-white-crane-white-belt/shared-vite",
  "version": "0.1.0",
  "dependencies": {
    "@ns-white-crane-white-belt/shared-core": "^0.1.0"
  },
  "devDependencies": {
    "@ns-white-crane-white-belt/vite-config-base": "^0.1.0",
    "@ns-white-crane-white-belt/vitest-config-node": "^0.1.0",
    "@ns-white-crane-white-belt/ts-config-lib-source": "^0.1.0",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "vite": "7.1.1",
    "vitest": "3.2.4"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./dist/src/index.d.ts",
      "node": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./*": {
      "import": "./src/*.ts",
      "types": "./dist/src/*.d.ts",
      "node": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "files": [
    "package.json",
    "dist"
  ],
  "main": "dist/src/index.js",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "coverage": "npm run coverage:vitest",
    "coverage:vitest": "vitest run --coverage",
    "fmt": "npm run fmt:prettier",
    "fmt:check": "npm run fmt:check:prettier",
    "fmt:check:prettier": "prettier --check .",
    "fmt:prettier": "prettier --write .",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:vitest",
    "test:vitest": "vitest run",
    "typecheck": "npm run typecheck:tsc",
    "typecheck:tsc": "tsc --noemit"
  },
  "type": "module"
}
