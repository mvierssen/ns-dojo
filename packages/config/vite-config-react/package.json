{
  "name": "@ns-dojo/vite-config-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "import": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "dependencies": {
    "@ns-dojo/vite-config-base": "0.1.0",
    "@vitejs/plugin-react": "5.1.1",
    "unplugin-auto-import": "20.2.0",
    "unplugin-fonts": "1.4.0",
    "unplugin-icons": "22.5.0"
  },
  "devDependencies": {
    "@ns-dojo/ts-config-lib-react-source": "0.1.0",
    "@ns-dojo/eslint-config-base": "0.1.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc --noemit",
    "test": "echo 'No tests for config package'"
  }
}
