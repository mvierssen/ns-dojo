{
  "name": "@ns-white-crane-white-belt/eslint-config-base",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "node": "./dist/src/index.js",
      "import": {
        "@ns-white-crane-white-belt/source": "./src/index.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "node": "./dist/src/*.js",
      "import": {
        "@ns-white-crane-white-belt/source": "./src/*.ts",
        "default": "./dist/src/*.js"
      }
    }
  },
  "dependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.36.0",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitest/eslint-plugin": "1.3.10",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-functional": "9.0.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.1.0",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "61.0.2",
    "globals": "16.4.0",
    "typescript-eslint": "8.46.0"
  },
  "devDependencies": {
    "@ns-white-crane-white-belt/ts-config-build": "^0.1.0",
    "@types/eslint": "9.6.1",
    "@types/eslint-plugin-security": "3.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  }
}
