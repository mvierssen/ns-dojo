{
  "name": "@ns-dojo/template-astro",
  "version": "0.1.0",
  "dependencies": {},
  "devDependencies": {
    "@astrojs/check": "0.9.5",
    "@ns-dojo/astro-config-react-cloudflare": "0.1.0",
    "@ns-dojo/eslint-config-astro-react": "0.1.0",
    "@ns-dojo/prettier-config": "0.1.0",
    "@ns-dojo/ts-config-astro": "0.1.0",
    "@ns-dojo/vitest-config-react": "0.1.0",
    "@testing-library/jest-dom": "6.9.1",
    "astro": "5.15.9",
    "typescript": "5.9.3",
    "unplugin-fonts": "1.4.0",
    "unplugin-icons": "22.5.0",
    "wrangler": "4.49.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "node": "./dist/src/index.js",
      "import": {
        "source": "./src/index.ts",
        "default": "./src/index.ts"
      }
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "node": "./dist/src/*.js",
      "import": {
        "source": "./src/*.ts",
        "default": "./src/*.ts"
      }
    }
  },
  "files": [
    "package.json",
    "dist"
  ],
  "main": "dist/src/index.js",
  "private": true,
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "ci:lint": "glab ci lint .gitlab-ci.yml",
    "coverage": "npm run coverage:vitest",
    "coverage:vitest": "vitest run --coverage",
    "deploy-production": "astro check && astro build && wrangler deploy --env production",
    "deploy-staging": "astro check && astro build && wrangler deploy --env staging",
    "dev": "astro dev",
    "fmt": "npm run fmt:prettier",
    "fmt:check": "npm run fmt:check:prettier",
    "fmt:check:prettier": "prettier --check .",
    "fmt:prettier": "prettier --write .",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "npm run test:vitest",
    "test:vitest": "vitest run",
    "typecheck": "npm run typecheck:tsc",
    "typecheck:tsc": "tsc --noemit",
    "wrangler:types": "wrangler types"
  },
  "type": "module"
}
